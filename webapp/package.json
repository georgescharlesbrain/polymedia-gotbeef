{
    "name": "beef-webapp",
    "private": true,
    "dependencies": {
        "react": "18",
        "react-dom": "18",
        "react-router-dom": "6"
    },
    "devDependencies": {
        "@parcel/transformer-less": "2.7.0",
        "parcel": "2",
        "process": "^0.11.10"
    },
    "scripts": {
        "serve": "parcel src/index.html",
        "build": "rm -rf dist/ && parcel build --no-source-maps src/index.html",
