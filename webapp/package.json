{
    "name": "gotbeef-webapp",
    "author": "juzybits <https://twitter.com/juzybits>",
    "private": true,
    "dependencies": {
        "@mysten/sui.js": "latest",
        "@mysten/wallet-adapter-sui-wallet": "latest",
        "js-confetti": "latest",
        "nes.css": "latest",
        "react": "18",
        "react-dom": "18",
        "react-router-dom": "6"
    },
    "devDependencies": {
        "@parcel/transformer-less": "latest",
        "parcel": "latest",
        "process": "latest",
        "react-devtools": "latest"
